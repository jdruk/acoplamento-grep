          strncpy (errbuf, msg, errbuf_size - 1);
          errbuf[errbuf_size - 1] = 0;
/* Free dynamically allocated space used by PREG.  */
  if (preg->buffer != NULL)
    free (preg->buffer);
  preg->buffer = NULL;
